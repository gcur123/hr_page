<%= form_for :holiday, url: holidays_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :start_date %><br>
    <%= f.text_field :start_date %>
  </p>
  <p>
    <%= f.label :end_date %><br>
    <%= f.text_field :end_date %>
  </p>
  <p>
    <%= f.label :number_of_days %><br>
    <%= f.text_field :number_of_days %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>